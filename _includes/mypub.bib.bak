@techreport{Andreotti2011,
abstract = {Heart diseases are the cause of many deaths during prenatal, childbirth and newborn periods. Therefore, the measurement of the fetal heart activity is of great importance in assessing the health state of the fetus. In order to evaluate the fetal state, health specialists, nowadays, mostly use the fetal heart rate (fHR) parameter. Many techniques for obtaining the fHR are available, however the reliability and exibility of these techniques is still not ideal. The fetal electrocardiogram (fECG) through abdominal electrodes appears to be a suitable solution to the problematic, which provides long-term monitoring during the prenatal and childbirth period. The dificulty of the fECG technique is the low energy of the fetal signals, which often is buried into maternal electrocardiography, electromyographic noises, between other noise sources. This presented study contemplates the implementation of a method to extract the fHR information from abdominal measurements. Hence, the robust Event-Synchronous Canceller (ESC) algorithm, based on template subtraction, is implemented. ESC's performance is accordingly tested with simulated and real data. Its capability for during long-term monitoring is proven and further analysis are discussed. The method was capable of detecting the fetal heart beats with an average accuracy of 82{\%} when simulated data for the different recording conditions were used. Extracting the fECG from abdominal signals is a recent development in the biomedical area. Moreover, it is the first work of the biomedical engineering research group at the Technische Universitaet Dresden in the subject. The result of this work should allow several other studies on the topic of the abdominal signals processing, mainly, the ones which depend on the acquisition of the fHR information.},
address = {Dresden},
author = {Andreotti, Fernando},
file = {:local/engs1314/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Andreotti - 2011 - Extraction of the Fetal ECG from Electrocardiographic Long-term Recordings.pdf:pdf},
institution = {TU Dresden},
keywords = {FHRV,Kalman filter},
mendeley-groups = {Mine,IBMT - TU Dresden,M.Sc. Thesis},
mendeley-tags = {FHRV,Kalman filter},
month = {aug},
pages = {99},
title = {{Extraction of the Fetal ECG from Electrocardiographic Long-term Recordings}},
year = {2011}
}
@inproceedings{Montemezzo2010c,
abstract = {The measurement of the maximal respiratory pressures (MRP) is a method used to evaluate the respiratory muscle strength. The aim of this study was to evaluate the influence of two different interfaces on subject´s capacity to generate MRP. Fifty healthy subjects with normal spirometry were evaluated. To measure the MRP a digital manovacuometer was used with two different interfaces at each time. A paired t-test, or Wilcoxon Test, was used to compare the interfaces, with p{\textless}0.05 to be considered significant. For the values of Maximal Inspiratory Pressure (MIP) and Maximal Expiratory Pressure (MEP) there were no significant differences comparing the two interfaces (p?0.265 and p?0.268 respectively). The results demonstrated that there were no significant differences in MIP and MEP values, indicating no influence of the two interfaces evaluated. Palavras-ch},
address = {Tiradentes, Brazil},
author = {Montemezzo, D. and Fernandes, A.A. and Souza-Cruz, A. A. and Fernandes, G. V. M. and Andreotti, F. and Barbosa, M. H. and Parreira, V. F. and Tierra-Criollo, C. J.},
booktitle = {Congresso Brasileiro de Engenharia Biomedica},
file = {:local/engs1314/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Montemezzo et al. - 2010 - Influencia de interfaces na mensuracao das pressoes respiratorias maximas.pdf:pdf},
mendeley-groups = {Mine},
pages = {1187--1190},
title = {{Influencia de interfaces na mensuracao das pressoes respiratorias maximas}},
year = {2010}
}
@inproceedings{Fernandes2010,
abstract = {For a correct diagnosis in medical area, a reliable measuring equipment is essential. Otherwise, patient's treatment and health might be seriously compromised. This article presents the application of a calibration protocol, evaluation of how often calibration must be made and analysis of a biological filter`s influence in a digital manovacuometer developed at UFMG. The use of this protocol, based on the least squares method and linear regression, has shown that the device requires annual calibrations and that use of this biological filter does not interfere on its measurements.},
address = {Tiradentes, Brazil},
author = {Fernandes, G.V.M. and Andreotti, Fernando and Montemezzo, D. and Souza-Cruz, A. A. and Parreira, V. F. and Tierra-Criollo, C.J.},
